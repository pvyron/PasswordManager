@attribute [Route(ApplicationRoutes.Dashboard)]

<PageTitle>Dashboard</PageTitle>



<div style="margin-top:2%; width:98%">


    <MudGrid Justify="Justify.SpaceEvenly" Spacing="10">
        
        @foreach (var password in Passwords)
        {
            <MudItem xl="3" lg="4" md="6" sm="12" xs="12">
                <PasswordManager.Portal.Components.PasswordCard Password="@password" OnDeletePassword="@OnDeletePasswordClicked" OnFavoriteChanged="@OnFavoriteChanged" OnViewPasswordCredentials="@OnViewPasswordCredentials" />
            </MudItem>
        }

    </MudGrid>

</div>