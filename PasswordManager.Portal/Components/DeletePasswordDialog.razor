<MudDialog>
    <DialogContent>
        <MudText Align="Align.Center" Color="Color.Dark" Typo="Typo.body2">@Message</MudText>
    </DialogContent>
    <DialogActions>
        <MudButton Color="Color.Error" OnClick="Submit">Delete</MudButton>
        <MudButton Color="Color.Primary" OnClick="Cancel">Cancel</MudButton>
    </DialogActions>
</MudDialog>
