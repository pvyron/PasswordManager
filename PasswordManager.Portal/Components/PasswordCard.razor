<MudCard>
    <MudCardHeader>
        <CardHeaderContent>
            <MudExpansionPanels Dense="true" DisableBorders="true" Elevation="0" Square="false">
                <MudExpansionPanel Text="@Password.Title" @bind-IsExpanded="@Password.IsDesciptionExpanded">
                    @Password.Description
                </MudExpansionPanel>
            </MudExpansionPanels>
        </CardHeaderContent>
        <CardHeaderActions>
            <MudIconButton Icon="@Icons.Material.Filled.Settings" Color="Color.Default" />
        </CardHeaderActions>
    </MudCardHeader>
@*       <MudCardMedia Image="images/pilars.jpg" Height="250" />*@
    <MudCardContent>
        <MudText Typo="Typo.body2">Username: @Password.Username</MudText>
        <MudText Typo="Typo.body2">Password: @Password.Password</MudText>
    </MudCardContent>
</MudCard>